<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.medic_schedule">

    <permission
        android:name="com.example.asus.medic_schedule.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.asus.medic_schedule.permission.MAPS_RECEIVE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- Required to show current location -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <!-- Required OpenGL ES 2.0. for Maps V2 -->
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/list"
        android:label="@string/app_name"
        android:theme="@style/myTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/nLiveoDrawer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Patient"
            android:label="Patient" />
        <activity
            android:name=".Doctor"
            android:label="Doctor" />
        <activity
            android:name=".Datalist_p"
            android:label="Patient Data"
            android:theme="@android:style/Theme.Holo.Light.DarkActionBar" />
        <activity
            android:name=".Datalist_d"
            android:label="DoctorData"
            android:theme="@android:style/Theme.Holo.Light.DarkActionBar" />
        <activity
            android:name=".Medicine"
            android:label="Medicine" />
        <activity
            android:name=".Datalist_m"
            android:label="Medicine Data"
            android:theme="@android:style/Theme.Holo.Light.DarkActionBar" />
        <activity
            android:name=".SettingsActivity"
            android:label="Medic-Schedule" />
        <activity
            android:name=".BloodPressure"
            android:label="Blood Pressure" />
        <activity
            android:name=".DataList_bp"
            android:label="Blood Pressure Data"
            android:theme="@android:style/Theme.Holo.Light.DarkActionBar" />
        <activity
            android:name=".RowItem_bp"
            android:label="Blood Pressure Data" />
        <activity
            android:name=".PlaceDetailsActivity"
            android:label="Places" />
        <activity
            android:name=".Places"
            android:label="Places" />

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyAV3WG8YnuLJDN8F6qGDF6f5jQNIt8deng" />

        <activity
            android:name=".extra"
            android:label="@string/title_activity_extra"
            android:theme="@style/myTheme.NoActionBar"></activity>
    </application>

</manifest>